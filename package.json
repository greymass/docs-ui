{
  "name": "@eosnetwork/docs-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 3001",
    "start:zh": "npm run start -- --locale zh",
    "start:ko": "npm run start -- --locale ko",
    "build": "node scripts/prepare-docs.js --skip-manuals && docusaurus build && node scripts/finalize-build.js",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --port 3001",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "translate": "node scripts/translations/translate"
  },
  "dependencies": {
    "@docusaurus/core": "2.4.0",
    "@docusaurus/plugin-client-redirects": "^2.4.0",
    "@docusaurus/plugin-sitemap": "^2.4.0",
    "@docusaurus/preset-classic": "2.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "docusaurus-plugin-image-zoom": "^1.0.1",
    "dotenv": "^16.1.4",
    "ethers": "^5.7.2",
    "isomorphic-fetch": "^3.0.0",
    "javascript-time-ago": "^2.5.9",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-select": "^5.7.3",
    "redocusaurus": "^1.6.2",
    "yauzl-promise": "^4.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-translate": "^3.348.0",
    "@aws-sdk/credential-providers": "^3.348.0",
    "@docusaurus/module-type-aliases": "2.4.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "autoprefixer": "^10.4.14",
    "aws-sdk": "^2.1417.0",
    "chokidar": "^3.5.3",
    "cli-progress": "^3.12.0",
    "fs-extra": "^11.1.1",
    "js-yaml": "^4.1.0",
    "postcss": "^8.4.24",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
